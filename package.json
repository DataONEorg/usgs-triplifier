{
  "name": "usgs-triplifier",
  "version": "1.0.0",
  "description": "Official USGS triplification software",
  "author": {
    "name": "Blake Regalia",
    "email": "blake.regalia@gmail.com"
  },
  "main": "lib/main/module.js",
  "keywords": [
    "linked-data",
    "semantic-web",
    "usgs",
    "gnis",
    "nhd",
    "dlg"
  ],
  "repository": "blake-regalia/usgs-triplifier",
  "scripts": {
    "preinstall": "git pull --force",
    "update": "npm i",
    "all": "npm run tnm && npm run gnis && npm run nhd",
    "gnis": "./tools/download-gnis.sh",
    "tnm": "node lib/tnm/download.js data/input/tnm/*.csv",
    "nhd": "node lib/gdb/extract lib/nhd/convert.js data/input/nhd/*.zip"
  },
  "devDependencies": {},
  "dependencies": {
    "JSONStream": "^1.3.2",
    "classer": "^1.3.0",
    "colors": "^1.1.2",
    "csv-parse": "^2.0.4",
    "mkdirp": "^0.5.1",
    "ogr2ogr": "^1.2.1",
    "pg": "^7.4.1",
    "pg-cursor": "^1.3.0",
    "progress": "^2.0.0",
    "request": "^2.83.0",
    "request-progress": "^3.0.0",
    "unzip": "^0.1.11",
    "worker": "^0.3.5",
    "yauzl": "^2.9.1"
  }
}
